{
  "semi": true,
  "tabWidth": 2,
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "none",
  "jsxBracketSameLine": true,
  "htmlWhitespaceSensitivity": "ignore",
  "bracketSameLine": true,
  "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-organize-imports"],
  "endOfLine": "auto"
}
